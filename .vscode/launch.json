{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug my_program (GDB)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/my_program",
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb",
            "externalConsole": false,
            "preLaunchTask": "Build with Make",
            "setupCommands": [
              {
                "description": "Enable pretty printing",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
              }
            ]
          }
          
    ]
}
